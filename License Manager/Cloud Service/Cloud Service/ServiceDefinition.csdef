<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Cloud_Service" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="DarkBond.LicenseManager.WorkerRole" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="APPINSIGHTS_INSTRUMENTATIONKEY" />
      <Setting name="Domain" />
      <Setting name="SqlConnectionString" />
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="DirectoryManagerAppPrincipalId" />
      <Setting name="DirectoryManagerSecret" />
      <Setting name="SignInPolicy" />
      <Setting name="TenantId" />
      <Setting name="Thumbprint" />
      <Setting name="Audience" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="WcfHttpPort" protocol="http" port="80" />
      <InputEndpoint name="WcfTcpPort" protocol="tcp" port="808" />
      <InputEndpoint name="WcfHttpsPort" protocol="https" port="443" certificate="Web Service Certificate" />
    </Endpoints>
    <Certificates>
      <Certificate name="Web Service Certificate" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
    <Startup>
      <Task commandLine="Setup.cmd" executionContext="elevated" taskType="simple" />
    </Startup>
  </WorkerRole>
</ServiceDefinition>