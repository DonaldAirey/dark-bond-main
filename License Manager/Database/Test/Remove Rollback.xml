  </transaction>
  <!-- Remove Country | Rollback -->
  <transaction>
    <method name="RemoveCountry">
      <parameter name="configurationId" value="Default" />
      <parameter name="countryKey" value="EA" />
    </method>
    <method name="RemoveCountry">
      <parameter name="configurationId" value="Fault" />
